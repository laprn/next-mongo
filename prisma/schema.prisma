datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model Post {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  isbn String
  title String
  url String
  purchasedDate String
  purchasedAt DateTime
}